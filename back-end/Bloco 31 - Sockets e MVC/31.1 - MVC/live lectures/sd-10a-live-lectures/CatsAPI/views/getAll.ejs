<html>
<head>
  <title>Gatos</title>
</head>
<body>
  
  <% if (message) { %>
    <h1><%= message %></h1>
  <% } %>

  <% cats.forEach(cat => { %>
    <li><a href="/cats/<%= cat.id %>"><%= cat.name %></a></li>
  <% }) %>

  <ul style="background-color: antiquewhite">
  </ul>
  <form action="/cats" method="POST">
    <input name="name" type="text">
    <input name="age" type="number">
    <button type="submit">Criar novo gato!</button>
  </form>
</body>
</html>